{
  "name": "movil",
  "version": "1.1.0",
  "description": "Proyecto base para NativeScript-Vue",
  "author": "Ciris Informatic Solutions <ventas@ciriscr.com>",
  "scripts": {
    "lint": "eslint src --ext js,vue --fix --cache",
    "prelint:watch": "npm run lint",
    "lint:watch": "onchange 'src/*.js' 'src/**/*.js' 'src/*.vue' 'src/**/*.vue' -- npm run lint",
    "build": "cross-env NODE_ENV=production webpack --env.tnsAction build",
    "build:android": "cross-env NODE_ENV=production npm run build -- --env.android",
    "build:ios": "cross-env NODE_ENV=production npm run build -- --env.ios",
    "debug": "cross-env DEBUG=ciris:* webpack --watch --env.tnsAction debug",
    "debug:android": "cross-env DEBUG=ciris:* npm run debug -- --env.android",
    "debug:ios": "cross-env DEBUG=ciris:* npm run debug -- --env.ios",
    "watch": "cross-env DEBUG=ciris:* webpack --watch --env.tnsAction run",
    "watch:android": "cross-env DEBUG=ciris:* npm run watch -- --env.android",
    "watch:ios": "cross-env DEBUG=ciris:* npm run watch -- --env.ios",
    "start": "npm-run-all --parallel debug:android ",
    "test": "cross-env DEBUG=ciris:* tape ./src/*.spec.js -r ./config/tests.js | tap-notify | tap-summary",
    "coverage": "cross-env DEBUG=ciris:* nyc  babel-tape-runner ./src/**/*.js | tap-nyc; nyc report --reporter=text-summary",
    "clean": "rimraf dist"
  },
  "engines": {
    "node": "~8.9.x",
    "npm": "~5.7.x",
    "cpu": [
      "x64",
      "ia32"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "axios": "0.18.0",
    "babel-tape-runner": "2.0.1",
    "eslint-plugin-lodash": "2.7.0",
    "nativescript-camera": "4.0.0",
    "nativescript-geolocation": "4.2.3",
    "nativescript-vue": "1.3.0",
    "npm-run-all": "4.1.2",
    "tns-core-modules": "3.4.1",
    "vue-axios": "2.0.2",
    "vue-progressbar": "0.7.4",
    "vue-router": "3.0.1",
    "vuex": "3.0.1"
  },
  "devDependencies": {
    "@vue/test-utils": "1.0.0-beta.10",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "chalk": "2.3.0",
    "copy-webpack-plugin": "4.4.2",
    "cross-env": "5.1.3",
    "css-loader": "0.28.10",
    "eslint": "4.15.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-vue": "4.1.0",
    "extract-text-webpack-plugin": "3.0.2",
    "fs-extra": "5.0.0",
    "nativescript-vue-externals": "0.1.1",
    "nativescript-vue-loader": "0.1.5",
    "nativescript-vue-target": "0.1.0",
    "nativescript-vue-template-compiler": "1.3.0",
    "node-sass": "4.7.2",
    "ns-vue-loader": "0.1.2",
    "optimize-css-assets-webpack-plugin": "3.2.0",
    "opn": "5.1.0",
    "ora": "1.3.0",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "semver": "5.4.1",
    "shelljs": "0.7.8",
    "tap-summary": "4.0.0",
    "tape": "4.8.0",
    "vue-template-compiler": "2.5.13",
    "webpack": "3.11.0",
    "webpack-synchronizable-shell-plugin": "0.0.6",
    "winston-color": "1.0.0"
  }
}
